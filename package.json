{
  "name": "aisearchengine",
  "version": "1.0.0",
  "author": "Paarisena",
  "description": "AI-powered stock analysis tool",
  "type": "module",
  "main": "electron/main.mjs",
  "scripts": {
    "dev": "next dev --webpack",
    "build": "next build --webpack",
    "start": "next start",
    "postinstall": "node scripts/install-electron.js",
    "electron": "electron .",
    "electron:dev": "electron .",
    "standalone": "node scripts/standalone.js",
    "app": "concurrently \"npm run dev\" \"wait-on http://localhost:3000 && electron .\"",
    "dist:win": "pnpm build && pnpm standalone && electron-builder --win --x64",
    "pack": "pnpm build && electron-builder --dir",
    "setup-credentials": "node scripts/setup-credentials.js",
    "sync-credentials": "node scripts/sync-credentials.js"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@iarna/toml": "^2.2.5",
    "@modelcontextprotocol/sdk": "^1.26.0",
    "@napi-rs/canvas": "^0.1.91",
    "axios": "^1.13.5",
    "cheerio": "^1.2.0",
    "dotenv": "^17.2.4",
    "express-rate-limit": "^8.2.1",
    "firebase": "^12.9.0",
    "framer-motion": "^12.34.0",
    "groq-sdk": "^0.37.0",
    "helmet": "^8.1.0",
    "lucide-react": "^0.553.0",
    "mongodb": "^7.1.0",
    "mongoose": "^8.23.0",
    "next": "16.1.6",
    "node-fetch": "^3.3.2",
    "openai": "^6.20.0",
    "pdf-parse": "^2.4.5",
    "pdf2json": "^4.0.2",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.13.0",
    "recharts": "^3.7.0",
    "rss-parser": "^3.13.0",
    "validator": "^13.15.26",
    "vitest": "^4.0.18",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@electron/get": "^4.0.2",
    "@next/swc-win32-x64-msvc": "16.1.6",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/debug": "^4.1.12",
    "@types/electron-is-dev": "^1.1.3",
    "@types/estree-jsx": "^1.0.5",
    "@types/hast": "^3.0.4",
    "@types/iarna__toml": "^2.0.5",
    "@types/mdast": "^4.0.4",
    "@types/ms": "^2.1.0",
    "@types/node": "^25.2.3",
    "@types/pdf-parse": "^1.1.5",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@types/unist": "^3.0.3",
    "@types/validator": "^13.15.10",
    "@vitejs/plugin-react": "^5.1.4",
    "baseline-browser-mapping": "^2.9.19",
    "electron": "^40.2.1",
    "electron-builder": "^26.7.0",
    "electron-is-dev": "^3.0.1",
    "eslint": "^9.39.2",
    "eslint-config-next": "16.1.6",
    "extract-zip": "^2.0.1",
    "fs-extra": "^11.3.3",
    "happy-dom": "^20.6.0",
    "jsdom": "^27.4.0",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3"
  },
  "build": {
    "appId": "com.aisearchengine.app",
    "productName": "AIStockAnalyzer",
    "directories": {
      "output": "release"
    },
    "asar": false,
    "files": [
      "electron/**/*",
      "DB/**/*",
      ".next/standalone/**/*",
      ".next/static/**/*",
      "public/**/*",
      "package.json",
      ".env.local",
      "node_modules/**/*",
      "node.exe",
      "!node_modules/**/test/**",
      "!node_modules/**/tests/**",
      "!node_modules/**/__tests__/**",
      "!node_modules/**/*.test.js",
      "!node_modules/**/*.spec.js",
      "!node_modules/**/*.md",
      "!node_modules/**/README*",
      "!node_modules/**/LICENSE*",
      "!node_modules/**/CHANGELOG*",
      "!node_modules/**/.bin",
      "!node_modules/**/*.map",
      "!**/*.map",
      "!**/.DS_Store"
    ],
    "asarUnpack": [
      "**/*.node",
      "**/node_modules/next/**",
      "**/node_modules/react/**",
      "**/node_modules/react-dom/**",
      "**/node_modules/styled-jsx/**",
      "**/node_modules/scheduler/**",
      "**/node_modules/@swc/**",
      "**/node_modules/@next/**",
      "**/node_modules/@esbuild/**",
      "**/node_modules/@napi-rs/**",
      "**/node_modules/mongoose/**",
      "**/node_modules/mongodb/**",
      "**/node_modules/bson/**",
      "**/node_modules/pdf-parse/**",
      "**/node_modules/@google/generative-ai/**",
      "**/node_modules/client-only/**",
      "**/node_modules/server-only/**",
      "**/node_modules/nanoid/**",
      "**/node_modules/postcss/**",
      "**/node_modules/sharp/**",
      ".env.local"
    ],
    "win": {
      "target": "nsis",
      "icon": "public/icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "deleteAppDataOnUninstall": true,
      "differentialPackage": true
    },
    "electronDownload": {
      "cache": "./electron-cache"
    }
  }
}
